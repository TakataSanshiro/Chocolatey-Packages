    <id>joytokey</id>
    <version>6.9.2</version>
    <title>Joy To Key</title>
    <authors>JTKSOFT</authors>
    <owners>Anthony Mastrean, Sanshiro</owners>
    <licenseUrl>https://joytokey.net/en/purchase</licenseUrl>
