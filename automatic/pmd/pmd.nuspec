    <projectSourceUrl>https://github.com/pmd/pmd</projectSourceUrl>
    <bugTrackerUrl>https://github.com/pmd/pmd/issues</bugTrackerUrl>
    <docsUrl>https://docs.pmd-code.org/latest/</docsUrl>
    <releaseNotes>https://github.com/pmd/pmd/releases</releaseNotes>
    <mailingListUrl>https://pmd.github.io/#support</mailingListUrl>
    <tags>source code analyze lint</tags>
    <licenseUrl>https://github.com/pmd/pmd/blob/6d4e902a2a36df937bb445a53223485a2e16e473/LICENSE</licenseUrl>
